@*                            <li>@Html.ActionLink("In Type", "Index", "InType", new { Area = "Admission" }, new { })</li>
                            <li>@Html.ActionLink("In Cat", "Index", "InCat", new { Area = "Admission" }, new{})</li>
                            <li>@Html.ActionLink("In Sub Cat", "Index", "InSubCat", new { Area = "Admission" }, new { })</li>
                            <li>@Html.ActionLink("In", "Index", "AjaxIn", new { Area = "Admission" }, new { })</li>*@
