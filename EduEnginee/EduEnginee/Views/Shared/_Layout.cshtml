                            <li>@Html.ActionLink("News", "Index", "AjaxNews", new { Area = "Admission" }, new { })</li>
                            <li>@Html.ActionLink("Notice", "Index", "AjaxNotice", new { Area = "Admission" }, new { })</li>
                            <li>@Html.ActionLink("Circular", "Index", "Circular", new { Area = "Admission" }, new { })</li>*@
                            <li>@Html.ActionLink("Dash Board", "Index", "Dashboard", new { Area = "Admission" }, new { })</li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - My ASP.NET MVC Application</p>
                </div>
                <div class="float-right">
                    <ul id="social">
                        <li><a href="http://facebook.com" class="facebook">Facebook</a></li>
                        <li><a href="http://twitter.com" class="twitter">Twitter</a></li>
                    </ul>
                </div>
            </div>
        </footer>
    </body>
</html>
