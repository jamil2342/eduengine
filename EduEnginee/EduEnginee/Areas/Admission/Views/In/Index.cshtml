@model IEnumerable<EduEnginee.Areas.Admission.Models.InstituteType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
@if (User.IsInRole("admin"))
{
    <p>@Html.ActionLink("Create New", "Create","In")</p>    
}
<div id="accordion" class="accordin">
    @foreach (var item in Model)
    {
                
        <h3>
            <a href="#">@item.Title</a></h3>
        <div>
            <p>
                @Html.Action("_insCat",new {InsTypeId=item.Id})
            </p>
        </div>
    
    }
</div>
