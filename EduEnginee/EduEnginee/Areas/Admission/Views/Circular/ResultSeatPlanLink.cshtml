@model IEnumerable<EduEnginee.Areas.Admission.Models.Circular>
<table >
    <tr>
        <th  width="50%">
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            Result
        </th>
        <th>
            Seat plan
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td width="50%">@Html.ActionLink(item.Title, "Details", new { id = item.Id })
            </td>
            <td>
                <a target="_blank" href="@item.ResultLink"><span style="color: #0000EE; padding-left: 0px;">
                    View Result</span></a>
            </td>
            <td>
                <a target="_blank" href="@item.SeatPlanLink"><span style="color: #0000EE; padding-left: 0px;">
                    View Seat Plan</span></a>
            </td>
        </tr>
    }
</table>
