<table width="100%" cellspacing="0" cellpadding="0" border="0" align="center" style="border:1px solid #D0CCC9;">
		<tbody><tr>
			<td colspan="2">

			</td>
		</tr>
		<tr>
			<td width="100%" align="center">
				<div class="social-plugin-box">
	<table cellspacing="0" cellpadding="0" border="0" align="left" style="border:0px solid #ffffff;" class="social-plugin">
		<tbody>
		<tr>
			<td valign="top" align="center">
				<br>
				<table width="100%" align="center">
					<tbody><tr>
						<td width="100%" valign="top" align="center" style="padding-bottom:30px; padding-top:10px;">
						<div align="center">
							<font style="font-size: 20px;">Admission Circular</font><br>
							<font style="font-size: 14px;">####admissionType,&nbsp;@Model.Title,&nbsp;Session: @Model.Session</font>
						</div>
						</td>
					</tr>
				</tbody></table>

			</td>
		</tr>
		<tr>
			<td>
				<div style="text-align: left;" class="heading2">Admission Requirements </div>
                <p>
                    @Model.AdmissionReq
                </p>              

			</td>
		</tr>
		<tr>
			<td>
				<table width="100%" align="center">
					<tbody><tr>
						<td width="65%" valign="top">
							<div style="text-align: center;" class="heading2">Subjects Under Common Unit</div>
							@foreach (var item in Model.Subjects)
                               {
                                   @item.Title 
                                    @Html.Encode("</br>")                                     
                                        
                                        
                               }
                        </td>
						<td width="35%" valign="top">
							<table width="300" cellspacing="0" cellpadding="0" border="0" align="center" class="containingbox">
								<tbody><tr>
									<td width="40%" valign="top" style="padding-left:6px;" class="related">
										<div class="heading11">Important Info</div>
											<div class="heading10">
												Start Date: <font color="#575757">@Model.AppStart</font><br>
												End Date: <font color="#575757">@Model.AppEnd</font><br>
												Admission Test: <font color="#575757">@Model.AddDate</font><br>
																									Helpline: <font color="#575757">@Model.Institute.Phone</font><br>
																									More Info: <font color="#575757">@Model.Institute.WebSite</font><br>
																							</div>	
									</td>
									<!--<td class="middle-middle">
										<h3>Important Info:</h3>
										Start Date: <font color="#575757"></font><br>
