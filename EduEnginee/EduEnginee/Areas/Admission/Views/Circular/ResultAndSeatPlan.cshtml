@model IEnumerable<EduEnginee.Areas.Admission.Models.Institute>
@{
    ViewBag.Title = "ResultAndSeatPlan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="accordion" class="accordin">
    @foreach (var item in Model)
    {
                
        <h3>
            <a href="#">@item.Title</a></h3>
        <div>
            
                @Html.Action("ResultSeatPlanLink", new { instituteId = item.Id })
            
        </div>
    
    }
</div>
