@model IEnumerable<EduEnginee.Areas.Visa.Models.VisaProcedureSet>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Visafee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequiredDocument)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Where_toApply)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpecialNote)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhotographyInformation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BasicRequirement)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisaTypeSet.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CountrySet.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Visafee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequiredDocument)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Where_toApply)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpecialNote)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhotographyInformation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BasicRequirement)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisaTypeSet.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CountrySet.Title)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
